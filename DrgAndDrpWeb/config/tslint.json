{
      "rulesDirectory": [
            "../node_modules/codelyzer"
      ],
      "rules": {
            "class-name": true,
            "component-class-suffix": true,
            "component-selector": [
                  true,
                  "element",
                  "composite2",
                  "kebab-case"
            ],
            "comment-format": [
                  true,
                  "check-space"
            ],
            "directive-class-suffix": true,
            "directive-selector": [
                  true,
                  "attribute",
                  "composite2",
                  "camelCase"
            ],
            "triple-equals": [
                  true,
                  "allow-null-check"
            ],
            "pipe-naming": [
                  true,
                  "camelCase",
                  "composite2"
            ],
            "use-pipe-transform-interface": true,
            "variable-name": [
                  true,
                  "check-format",
                  "allow-leading-underscore",
                  "ban-keywords"
            ],
            "no-bitwise": false,
            "switch-default": true,
            "no-shadowed-variable": false,
            "no-var-requires": true,
            "indent": [
                  true,
                  "spaces",
                  2
            ],
            "interface-name": [
                  true,
                  "always-prefix"
            ],
            "no-unnecessary-class": [
                  true,
                  "allow-constructor-only",
                  "allow-static-only"
            ],
            "member-access": false,
            "member-ordering": [
                  true,
                  "public-before-private",
                  "static-before-instance",
                  "variables-before-functions"
            ],
            "quotemark": [
                  true,
                  "double",
                  "avoid-template",
                  "avoid-escape"
            ],
            "no-debugger": true,
            "no-unused-expression": [
                  true,
                  "allow-fast-null-Hchecks"
            ],
            "no-empty": true,
            "no-console": [
                  true,
                  "log",
                  "debug",
                  "info",
                  "time",
                  "timeEnd",
                  "trace"
            ],
            "no-implicit-dependencies": true,
            "eofline": true,
            "no-duplicate-variable": [
                  true,
                  "check-parameters"
            ],
            "no-duplicate-super": true,
            "max-file-line-count": [
                  true,
                  400
            ],
            "max-line-length": [
                  true,
                  120
            ],
            "align": [
                  true,
                  "parameters",
                  "statements"
            ],
            "semicolon": [
                  true,
                  "always",
                  "strict-bound-class-methods"
            ],
            "newline-before-return": true,
            "one-line": [
                  true,
                  "check-open-brace",
                  "check-whitespace",
                  "check-else",
                  "check-catch",
                  "check-finally"
            ],
            "no-consecutive-blank-lines": [
                  true,
                  1
            ],
            "space-before-function-paren": [
                  true,
                  {
                        "anonymous": "never",
                        "named": "never",
                        "asyncArrow": "always",
                        "method": "never",
                        "constructor": "never"
                  }
            ],
            "import-spacing": true,
            "no-duplicate-switch-case": true,
            "no-switch-case-fall-through": true,
            "use-life-cycle-interface": true,
            "whitespace": [
                  true,
                  "check-branch",
                  "check-decl",
                  "check-operator",
                  "check-separator",
                  "check-type"
            ],
            "typedef-whitespace": [
                  true,
                  {
                        "call-signature": "nospace",
                        "index-signature": "nospace",
                        "parameter": "nospace",
                        "property-declaration": "nospace",
                        "variable-declaration": "nospace"
                  }
            ]
      }
}